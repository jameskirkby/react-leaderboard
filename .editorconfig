# top-most EditorConfig file
root = true

# Unix-style newlines, 4 spaces for indentation and default charset of utf-8
[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4

# Tab indentation for makefile (no size specified, defaults to indent_size)
[Makefile]
indent_style = tab


# Tab indentation for makefile (no size specified, defaults to indent_size)
[*.js]
indent_size = 2